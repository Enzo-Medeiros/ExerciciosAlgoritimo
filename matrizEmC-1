#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void){
     
int mat[3][3];
int i,j;
        printf("Insira os numeros desejados na linha 1 da matriz: \n");
        for (i = 0; i < 3; i++)
        {
            scanf("%d", &mat[0][i]);
        }

        printf("Insira os numeros desejados na linha 2 da matriz: \n");
        for (i = 0; i < 3; i++)
        {
            scanf("%d", &mat[1][i]);
        }

        printf("Insira os numeros desejados na linha 3 da matriz: \n");
        for (i = 0; i < 3; i++)
        {
            scanf("%d", &mat[2][i]);
        }

        printf("A matriz inserida Ã© essa: \n");
        for (i = 0; i < 3; i++)
        {
            for (j = 0; j < 3; j++)
            {
                printf(" |%d| ", mat[i][j]);
            }
        printf("\n");
        }

}
