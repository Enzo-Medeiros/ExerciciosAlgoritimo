#include <stdio.h>
int main(void){
int num1, num2, num3;
     puts("Insira um numero inteiro:\n");
     scanf("%d", &num1);
     puts("Insira um numero inteiro:\n");
     scanf("%d", &num2);
     puts("Insira um numero inteiro:\n");
     scanf("%d", &num3);
     if(num1>num2&&num1>num3){
     printf("%d e o maior numero", num1);
     }
     else if(num2>num1&&num2>num3){
     printf("%d e o maior numero", num2);
     }
     else if(num3>num2&&num3>num1){
     printf("%d e o maior numero", num3);
     }
}
