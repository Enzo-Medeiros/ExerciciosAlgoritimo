#include <stdio.h>
int main(void){
int mat[3][3]={{1,2,3},{4,5,6},{7,8,9}};
int i, j, val;
    printf("Insira um valor para multiplicar a matriz: \n");
        scanf("%d", &val);
    printf("Resultado da multiplicação da matriz por %d:\n", val);
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            mat[i][j] *= val;
            printf("%d ", mat[i][j]);
        }
        printf("\n");
    }

    return 0;
}
